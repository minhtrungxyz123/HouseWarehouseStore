@model HouseWarehouseStore.Models.FollowModel

<div>
    <h4>Chi tiết Follow</h4>
    <hr />
    <form asp-controller="Follow" asp-action="Detail">
        <div class="form-body">
            <input type="hidden" asp-for="FollowId" />
            <div class="modal-body">
                <div class="form-group">
                    <label asp-for="FollowLink" class="control-label"></label>
                    <input asp-for="FollowLink" class="form-control" min="0" readonly="@(true)" />
                    <span asp-validation-for="FollowLink" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Icon" class="control-label"></label>
                    <input asp-for="Icon" class="form-control" min="0" readonly="@(true)" />
                    <span asp-validation-for="Icon" class="text-danger"></span>
                </div>
            </div>
        </div>
    </form>
</div>